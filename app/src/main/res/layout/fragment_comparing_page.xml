<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="@color/primary_color_white"
	android:orientation="vertical"
	android:paddingStart="90dp"
	android:paddingTop="100dp">


	<TextView
		android:id="@+id/nameTxtView"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:text="Lens Material"
		android:textColor="@color/primary_color_black"
		android:textSize="18dp"
		android:layout_marginTop="@dimen/compairing_top_margin_first_param"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent" />

	<com.facebook.drawee.view.SimpleDraweeView
		android:id="@+id/line1"
		android:layout_width="match_parent"
		android:layout_height="1dp"
		android:layout_marginTop="3dp"
		android:background="@color/primary_color_gray"
		app:layout_constraintTop_toBottomOf="@id/nameTxtView" />


	<TextView
		android:id="@+id/nameTxtView2"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:text="Benzel Material"
		android:textColor="@color/primary_color_black"
		android:textSize="18dp"
		android:layout_marginTop="3dp"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@id/line1" />

	<com.facebook.drawee.view.SimpleDraweeView
		android:id="@+id/line2"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginTop="3dp"
		android:background="@color/primary_color_gray"
		app:layout_constraintTop_toBottomOf="@id/nameTxtView2" />


	<include
		android:id="@+id/layoutComparingItem1"
		layout="@layout/layout_comparing_item"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"

		app:layout_constraintStart_toEndOf="@id/guideline2"/>

	<include
		android:id="@+id/layoutComparingItem2"
		layout="@layout/layout_comparing_item"
		android:layout_marginStart="30dp"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		app:layout_constraintStart_toEndOf="@id/layoutComparingItem1"/>

	<include
		android:id="@+id/layoutComparingItem3"
		layout="@layout/layout_comparing_item"
		android:layout_marginStart="30dp"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		app:layout_constraintStart_toEndOf="@id/layoutComparingItem2"/>

	<androidx.constraintlayout.widget.Guideline
		android:id="@+id/guideline2"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:orientation="vertical"
		app:layout_constraintGuide_begin="200dp" />


	<com.facebook.drawee.view.SimpleDraweeView
		android:id="@+id/addView"
		android:layout_width="88dp"
		android:layout_height="88dp"
		android:layout_marginEnd="70dp"
		android:rotation="45"
		android:layout_marginTop="40dp"
		android:background="@drawable/ic_remove_model"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>